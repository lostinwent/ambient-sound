}

void frequencyColor() {
  for(uint16_t i = 0; i < sizeof(frequency)-1; i++) {
    if (frequency[i] > 850 && frequency[i] <= 900) {
      for(int j = 0; j <= 3; j++) {
        strip.setPixelColor(j, 255, 250, 205); 
      }
    } else if (frequency[i] >900 && frequency[i] <= 950) {
      for(int j = 0; j <= 5; j++) {
        strip.setPixelColor(j, 255, 248, 220); 
      }
    } else if (frequency[i] >950 && frequency[i] <= 1000) {
      for(int j = 0; j <= 8; j++) {
          strip.setPixelColor(j, 245, 245, 220);
      }
    } else if (frequency[i] >1000 && frequency[i] <= 1200) {
      for(int j = 0; j <= 10; j++) {
          strip.setPixelColor(j, 255, 215, 0);
      }
    } else if (frequency[i] >1200 && frequency[i] <= 1400) {
      for(int j = 0; j <= 13; j++) {
          strip.setPixelColor(j, 255, 246, 143);
      }
    } else if (frequency[i] >1400 && frequency[i] <= 1700) {
      for(int j = 0; j <= 17; j++) {
          strip.setPixelColor(j, 255, 215, 0);
      }
    } else if (frequency[i] >1700 && frequency[i] <= 2000) {
      for(int j = 0; j <= 20 ; j++) {
          strip.setPixelColor(j, 255, 248, 220);
      }  
    } else if (frequency[i] >2000 && frequency[i] <= 2500) {
      for(int j = 0; j <= 24 ; j++) {
          strip.setPixelColor(j, 255, 165, 0);
      }  
